                        <a href="<%= about.links[1].url %>" target="_blank" class="fab fa-twitter"></a>
                        <a href=" <%= about.links[2].url %> " target="_blank" class="fab fa-instagram"></a>
                        <a href="<%= about.links[3].url %>" target="_blank" class="fab fa-pinterest"></a>
                    </div>
                </div>

            </div>
        </div>
