    <section class="about">
        <div class="container-fluid">
            <div class="row text-center">
                <h2 class="heading">about</h2>
                <div class="col-md-6 about-img">
                    <img src="https://cdn.pixabay.com/photo/2022/03/16/01/23/bird-7071408_960_720.jpg" class="img-fluid"
                        alt="">
                </div>
                <div class="col-md-6 about-text">
                    <h3 class="title">About us</h3>
                    <p><%= about.para1 %></p>
                    <p><%= about.para2 %></p>
                    <div class="icon">
