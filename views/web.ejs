<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/admin.css">

    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>admin panel</title>
</head>

<body>


<div class="container-fluid bg-light">

        <div class="container">
            <h2 class="alert alert-danger text-center">controller </h2>
            <table class="table table-striped table-hover">
                <thead>
                <h2> header </h2>
                  <tr>
                    <th scope="col">S.no</th>
                        <th scope="col">Name</th>
                        <th scope="col">endpoint</th>

                        <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                    <% var i=1 %>
                    <% var links = details.links %>
                        <tr>
                            <th scope="row"><%= i++ %></th>

                            <td><%= details.brandName %></td>
                            <% links.forEach(function(link){ %>
                                <th scope="row"><%= i++ %></th>
                                <td></td>

                                <td><%= link.url %></td>
                                
                                 
                           
                            
                            <td>
                                <form action="/admin/edit/<%= details._id %>" method="get" class="d-inline">
                                    <button class="btn" type="submit">
                                        <i class="fas fa-pen-square text-info mx-2"></i>
                                    </button>
                                </form>
                                <form action="/student/delete/<%= details._id %>" method="get" class="d">
                                    <button class="btn" type="submit">
                                        <i class="fas fa-trash text-danger"></i>
                                        
                                    </button>
                                </form>
                            </td>
                          </tr>
                          <%})%>

   
                           <th scope="row"><%= i + 1 %></th>

                            <% main.forEach((item)=>{%>
                                <td><%= item.title %></td>
                                <td><%= item.subtitle %></td>
                                <td><%= item.imageurl %></td>

                                
                                
                        
                          <td>
                              <form action="/student/edit/<%= item._id %>" method="get" class="d-inline">
                                  <button class="btn" type="submit">
                                      <i class="fas fa-pen-square text-info mx-2"></i>
                                  </button>
                              </form>
                              <form action="/student/delete/<%= item._id %>" method="get" class="d-inline">
                                  <button class="btn" type="submit">
                                      <i class="fas fa-trash text-danger"></i>
                                      
                                  </button>
                              </form>
                          </td>
                          <%})%>                   

                </tbody>
              </table>
              <table class="table table-striped table-hover">
                <thead>
                <h2> team section </h2>
                 
                  <tr>
                    <th scope="col">S.no</th>
                    <th scope="col">title</th>
                    <th scope="col">subtitle</th>
                    <th scope="col">imageurl</th>
                    <th scope="col">para1</th>
                    <th scope="col">para2</th>

                  </tr>
                </thead>
                <tbody>
                    <% var i=1 %>
                    <% team.forEach((item)=>{ %>
                        <tr>
                            <th scope="row"><%= i++ %></th>
                            <td><%= item.title %></td>
                            <td><%= item.subtitle %></td>
                            <td><%= item.imageurl %></td>
                            <td><%= item.para1 %></td>
                            <td><%= item.para2 %></td>

                            <td>
                                <form action="/student/edit/<%= item._id %>" method="get" class="d-inline">
                                    <button class="btn" type="submit">
                                        <i class="fas fa-pen-square text-info mx-2"></i>
                                    </button>
                                </form>
                                <form action="/student/delete/<%= item._id %>" method="get" class="d-inline">
                                    <button class="btn" type="submit">
                                        <i class="fas fa-trash text-danger"></i>
                                        
                                    </button>
                                </form>
                            </td>
                        <% })%>

                          </tr>
                </tbody>
              </table>
              <table class="table table-striped table-hover">
                <thead>
                <h2> product section </h2>
                 
                  <tr>
                    <th scope="col">S.no</th>
                    <th scope="col">title</th>
                    <th scope="col">subtitle</th>
                    <th scope="col">imageurl</th>
                    <th scope="col">para1</th>
                    <th scope="col">para2</th>
                    <th scope="col">action</th>


                  </tr>
                </thead>
                <tbody>
                <% var i = 1 %>
               <% products.forEach((product)=>{ %>
                <tr>
                    <th scope="row"><%= i++ %></th>
                    <td><%= product.title %></td>
                    <td><%= product.subtitle %></td>
                    <td><%= product.imageurl %></td>
                    <td><%= product.para1 %></td>
                    <td><%= product.para2 %></td>

                    <td>
                        <form action="/admin/edit/<%= product._id %>" method="get" class="d-inline">
                            <button class="btn" type="submit">
                                <i class="fas fa-pen-square text-info mx-2"></i>
                            </button>
                        </form>
                        <form action="/admin/delete/<%= product._id %>" method="get" class="d-inline">
                            <button class="btn" type="submit">
                                <i class="fas fa-trash text-danger"></i>
                                
                            </button>
                        </form>
                    </td>
                <% }) %>
                <table class="table table-striped table-hover">
                    <thead>
                    <h2> about</h2>
                      <tr>
                        <th scope="col">S.no</th>
                            <th scope="col">Name</th>
                            <th scope="col">endpoint</th>
    
                            <th scope="col">Action</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% var i=1 %>
                        <% var links = about.links %>
                            <tr>
                                <th scope="row"><%= i %></th>
    
                                <td><%= about.para1 %></td>
                                <td><%= about.para2 %></td>

                                <% links.forEach(function(link){ %>
                                    <th scope="row"><%= i++ %></th>
                                    <td></td>
    
                                    <td><%= link.url %></td>
                                    
                                     
                               
                                
                                <td>
                                    <form action="/admin/edit/<%= details._id %>" method="get" class="d-inline">
                                        <button class="btn" type="submit">
                                            <i class="fas fa-pen-square text-info mx-2"></i>
                                        </button>
                                    </form>
                                    <form action="/student/delete/<%= details._id %>" method="get" class="d">
                                        <button class="btn" type="submit">
                                            <i class="fas fa-trash text-danger"></i>
                                            
                                        </button>
                                    </form>
                                </td>
                              </tr>
                              <%})%>
                            
                              <table class="table table-striped table-hover">
                                <thead>
                                <h2>contact</h2>
                                  <tr>
                                    <th scope="col">S.no</th>
                                        <th scope="col">address</th>
                                        <th scope="col">Gmail</th>
                
                                        <th scope="col">phone no.</th>
                                  </tr>
                                </thead>
                                <tbody>
                                <% var i=1 %>
                                <% var cont = c_section %>
                                    <tr>
                                        <th scope="row"><%= i %></th>
            
                                        <td><%= cont.address%></td>
                                        <td><%= cont.gmail %></td>
                                        <td><%= cont.phone %></td>      
                                        <td>
                                            <form action="/admin/edit/<%= cont._id %>" method="get" class="d-inline">
                                                <button class="btn" type="submit">
                                                    <i class="fas fa-pen-square text-info mx-2"></i>
                                                </button>
                                            </form>
                                            <form action="/student/delete/<%= cont_id %>" method="get" class="d">
                                                <button class="btn" type="submit">
                                                    <i class="fas fa-trash text-danger"></i>
                                                    
                                                </button>
                                            </form>
                                        </td>
                                      </tr>
                                
    
                </tbody>
              </table>
        </div>
    </body>
        